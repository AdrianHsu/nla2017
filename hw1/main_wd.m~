% A1. example 9.3.1
% problem set a.
% A = [2 1 1; 1 2 1; 1 1 2];
% x0 = [1 -1 2]';


TOL = 1e-15;
maxIter = 100;

% problem set in Example 4, p.597
% A = [4 -1 1; -1 3 -2; 1 -2 3];
% x0 = [1 1 1]';
% A = [0 0; 1 -1];
A = [3 -1 -1 -1; -1 2 -1 0; -1 -1 3 -1; -1 0 -1 2];

x0 = [1 1 1 1]';

[eigval, eigvec] = pm(A, x0, TOL, maxIter);

x = [1; 1; 1];

[u1,r1] = wd(A,eigval,eigvec,x,TOL,maxIter);

x1=A(2,:)'/(r1*u1(2));

v1=(r1-0)*u1; %+ r0*(x0'*u1)*u0;
r = [];
v = [];
r = [r r1];
v = [v v1];


B2=A-r1*u1*x1'; 
[u2,r2] = wd(B2,r1,v1,x,TOL,maxIter);




% u =
% 
%    -2.0000
%    -1.0000
%     1.0000
% 
% 
% 
% mu =
% 
%     3.0000
